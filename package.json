{
  "name": "redis-scanner",
  "version": "0.1.0",
  "description": "A simple way to use Redis SCANs from Node.js",
  "author": {
    "name": "Isaac Whitfield",
    "email": "iwhitfield@appcelerator.com"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/iwhitfield/redis-scanner.git"
  },
  "bugs": "http://github.com/iwhitfield/redis-scanner/issues",
  "license": "MIT",
  "readmeFilename": "README.md",
  "keywords": [
    "redis",
    "redis-server",
    "scan",
    "sscan",
    "hscan",
    "zscan"
  ],
  "dependencies": {
    "async": "^0.9.0"
  },
  "devDependencies": {
    "blanket": "^1.1.6",
    "codeclimate-test-reporter": "^0.0.4",
    "grunt": "^0.4.5",
    "grunt-contrib-jshint": "^0.11.1",
    "grunt-mocha-cov": "^0.4.0",
    "grunt-mocha-test": "^0.12.7",
    "hiredis": "^0.3.0",
    "mocha": "^2.2.4",
    "mocha-lcov-reporter": "^0.0.2",
    "redis": "^0.12.1",
    "should": "^5.2.0"
  },
  "scripts": {
    "coverage": "grunt coverage",
    "lcov": "grunt lcov",
    "test": "grunt"
  },
  "config": {
    "blanket": {
      "data-cover-never": [
        "node_modules",
        "test"
      ],
      "pattern": [
        "index.js",
        "lib",
        "util"
      ]
    }
  }
}
